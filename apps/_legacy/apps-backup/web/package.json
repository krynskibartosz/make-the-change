{
  "name": "@make-the-change/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --turbo",
    "dev": "next dev --turbo",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "typegen": "next typegen",
    "build:analyze": "ANALYZE=true next build --turbo",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run test/unit",
    "test:integration": "vitest --run test/integration",
    "test:e2e": "playwright test",
    "test:ci": "vitest --run --coverage",
    "test:ui": "vitest --ui",
    "test:unit:ui": "vitest --ui test/unit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:tdd": "playwright test test/e2e/admin-subscriptions.spec.ts --headed --workers=1",
    "test:tdd:watch": "./scripts/tdd-dev.sh watch",
    "tdd:setup": "./scripts/tdd-dev.sh setup",
    "tdd:seed": "./scripts/tdd-dev.sh seed",
    "tdd:clean": "./scripts/tdd-dev.sh clean",
    "tdd:reset": "./scripts/tdd-dev.sh reset"
  },
  "dependencies": {
